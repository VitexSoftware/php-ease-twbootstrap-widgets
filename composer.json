{
    "name": "vitexsoftware/ease-twbootstrap-widgets",
    "description": "EasePHP framework addons and code snipplets",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ease\\TWB\\Widgets\\": "src/Ease/TWB/Widgets"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\TWB\\Widgets\\": "tests/src/Ease/ui",
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\Ease\\TWB\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease/TWB"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "vitexsoftware/ease-twbootstrap": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "type": "library",
    "homepage": "http://v.s.cz/ease.php",
    "license": "GPL-2.0+",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
